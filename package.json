{
  "name": "excella-central",
  "version": "1.0.0",
  "author": "Excella Labs",
  "homepage": "https://central.excellalabs.com/",
  "private": true,
  "imageRepo":
    "090999229429.dkr.ecr.us-east-1.amazonaws.com/excella-central-web",
  "imageName": "excella-central-web",
  "imagePort": "5000",
  "awsRegion": "us-east-1",
  "awsEcsCluster": "excella-prod-ecs-cluster",
  "awsService": "excella-central-web",

  "scripts": {
    "clean": "ionic-app-scripts clean",
    "build": "ionic-app-scripts build",
    "lint": "ionic-app-scripts lint",
    "ionic:build": "ionic-app-scripts build",
    "ionic:serve": "ionic-app-scripts serve",
    "test": "karma start ./test-config/karma.conf.js",
    "test-debug":
      "karma start ./test-config/karma.conf.js --auto-watch --no-single-run",
    "test-ci": "karma start ./test-config/karma.conf.js --single-run",
    "test-coverage": "karma start ./test-config/karma.conf.js --coverage",
    "e2e": "npm run e2e-update && npm run e2e-test",
    "e2e-test": "protractor ./test-config/protractor.conf.js",
    "e2e-update": "webdriver-manager update --standalone false --gecko false",

    "docker:build":
      "npm run build -- --prod && with-package docker image build . -t pkg.imageRepo:latest",
    "docker:run": "npm run docker:clean && npm run docker:runHelper",
    "docker:runHelper":
      "with-package docker run --name pkg.imageName -d -p pkg.imagePort:3000 pkg.imageRepo",
    "docker:publish": "with-package docker image push pkg.imageRepo:latest",
    "docker:clean": "with-package docker rm -f pkg.imageName || true",
    "docker:taillogs": "with-package docker logs -f pkg.imageName",
    "docker:debug":
      "npm run docker:build -- --file Dockerfile.dev && npm run docker:run && sleep 1 && with-package open 'http://localhost:pkg.imagePort' && npm run docker:taillogs",
    "aws:login":
      "with-package aws ecr get-login --no-include-email --region pkg.awsRegion",
    "aws:deploy":
      "with-package docker run --env-file ./.env silintl/ecs-deploy -c pkg.awsEcsCluster -n pkg.awsService -i pkg.imageRepo:latest -r pkg.awsRegion --timeout 1000",
    "aws:release":
      "npm run aws:login && npm run docker:publish && npm run aws:deploy"
  },
  "dependencies": {
    "@angular/common": "4.4.3",
    "@angular/compiler": "4.4.3",
    "@angular/compiler-cli": "4.4.3",
    "@angular/core": "4.4.3",
    "@angular/forms": "4.4.3",
    "@angular/http": "4.4.3",
    "@angular/platform-browser": "4.4.3",
    "@angular/platform-browser-dynamic": "4.4.3",
    "@cloudinary/angular-4.x": "^1.1.0",
    "@ionic-native/core": "4.3.0",
    "@ionic-native/splash-screen": "4.3.0",
    "@ionic-native/status-bar": "4.3.0",
    "@ionic/storage": "2.0.1",
    "cloudinary-core": "^2.4.0",
    "cordova-android": "6.4.0",
    "cordova-browser": "5.0.3",
    "cordova-ios": "4.5.4",
    "cordova-plugin-device": "^1.1.7",
    "cordova-plugin-ionic": "~2.0.4",
    "cordova-plugin-ionic-webview": "^1.1.16",
    "cordova-plugin-splashscreen": "^4.1.0",
    "cordova-plugin-statusbar":
      "https://github.com/apache/cordova-plugin-statusbar.git",
    "cordova-plugin-whitelist": "^1.3.3",
    "cordova-sqlite-storage": "^2.1.2",
    "ionic-angular": "3.7.1",
    "ionic-plugin-keyboard": "^2.2.1",
    "ionicons": "3.0.0",
    "rxjs": "5.4.3",
    "sw-toolbox": "3.6.0",
    "web": "0.0.2",
    "zone.js": "0.8.18"
  },
  "devDependencies": {
    "@ionic/app-scripts": "3.0.0",
    "@types/jasmine": "^2.8.0",
    "@types/node": "^8.0.51",
    "angular2-template-loader": "^0.6.2",
    "html-loader": "^0.5.1",
    "istanbul-instrumenter-loader": "^3.0.0",
    "jasmine": "^2.8.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.6",
    "null-loader": "^0.1.1",
    "protractor": "^5.2.0",
    "ts-loader": "^3.1.1",
    "ts-node": "^3.3.0",
    "typescript": "2.3.4"
  },
  "description": "",
  "cordova": {
    "platforms": ["ios", "android", "browser"],
    "plugins": {
      "ionic-plugin-keyboard": {},
      "cordova-plugin-whitelist": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-device": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-ionic-webview": {},
      "cordova-sqlite-storage": {},
      "cordova-plugin-ionic": {
        "APP_ID": "ec3bc509",
        "CHANNEL_NAME": "Production",
        "UPDATE_METHOD": "background"
      }
    }
  }
}
